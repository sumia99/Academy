@model IEnumerable<Slider>

<div class="container-fluid p-0 mb-5">
    <div class="owl-carousel header-carousel position-relative owl-loaded owl-drag">
        @foreach(var item in Model)
        {
        <div class="owl-stage-outer" >
            <div class="owl-stage" style="transform: translate3d(-2382px, 0px, 0px); transition: all 1.5s ease 0s; width: 4764px;">
                    <div class="owl-item cloned" style="width: 900px;">
                    <div class="owl-carousel-item position-relative">
                        <img class="img-fluid" src="img/carousel-1.jpg" alt="">
                        <div class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center" style="background: rgba(24, 29, 56, .7);">
                            <div class="container">
                                <div class="row justify-content-start">
                                    <div class="col-sm-10 col-lg-8">
                                        <h5 class="text-primary text-uppercase mb-3 animated slideInDown">Best Online Courses</h5>
                                        <h1 class="display-3 text-white animated slideInDown">@item.SliderTitle</h1>
                                        <p class="fs-5 text-white mb-4 pb-2">@item.SliderSubTitle</p>
                                            <a asp-action="Register" asp-controller="Account" class="btn btn-primary py-md-3 px-md-5 me-3 animated slideInLeft">Register</a>
                                            <a asp-action="Login" asp-controller="Account" class="btn btn-light py-md-3 px-md-5 animated slideInRight">Login</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
               
                <div class="owl-item" style="width: 794px;">
                    <div class="owl-carousel-item position-relative">
                        <img class="img-fluid" src="img/carousel-1.jpg" alt="">
                        <div class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center" style="background: rgba(24, 29, 56, .7);">
                            <div class="container">
                                <div class="row justify-content-start">
                                    <div class="col-sm-10 col-lg-8">
                                            <h1 class="display-3 text-white animated slideInDown">@item.SliderTitle</h1>
                                            <p class="fs-5 text-white mb-4 pb-2">@item.SliderSubTitle</p>
                                            <a asp-action="Register" asp-controller="Account" class="btn btn-primary py-md-3 px-md-5 me-3 animated slideInLeft">Register</a>
                                            <a asp-action="Login" asp-controller="Account" class="btn btn-light py-md-3 px-md-5 animated slideInRight">Login</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="owl-item active" style="width: 794px;">
                  
                </div>
                <div class="owl-item cloned" style="width: 794px;">
                    <div class="owl-carousel-item position-relative">
                        <img class="img-fluid" src="img/carousel-1.jpg" alt="">
                        <div class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center" style="background: rgba(24, 29, 56, .7);">
                            <div class="container">
                                <div class="row justify-content-start">
                                    <div class="col-sm-10 col-lg-8">
                                            <h1 class="display-3 text-white animated slideInDown">@item.SliderTitle</h1>
                                            <p class="fs-5 text-white mb-4 pb-2">@item.SliderSubTitle</p>
                                            <a asp-action="Register" asp-controller="Account" class="btn btn-primary py-md-3 px-md-5 me-3 animated slideInLeft">Register</a>
                                            <a asp-action="Login" asp-controller="Account" class="btn btn-light py-md-3 px-md-5 animated slideInRight">Login</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
            </div>
        </div><div class="owl-nav"><div class="owl-prev"><i class="bi bi-chevron-left"></i></div><div class="owl-next"><i class="bi bi-chevron-right"></i></div></div><div class="owl-dots disabled"></div>
        }
    </div>
</div>